<!--
val_0
val_p1
val_m1
val_baseInv
val_wtr
val_rivBtm
val_ocnBtm
val_mntHeight
-->
<configs>
<insertBefore xpath="/*/terrain_generators/*[@name='vanilla']/*[@name='val_0']">
	<module name="cyl_512" type="Cylinders">
		<property name="frequency" value="0.0009765625"/>
	</module>
	<module name="wav_z512" type="ScaleInput">
		<property name="sourceModule" value="cyl_512"/>
		<property name="x" value="1"/>
		<property name="y" value="0"/>
		<property name="z" value="0"/>
	</module>
	<module name="wav_x512" type="ScaleInput">
		<property name="sourceModule" value="cyl_512"/>
		<property name="x" value="0"/>
		<property name="y" value="0"/>
		<property name="z" value="1"/>
	</module>
	<module name="val_p0_35" type="Constant"><property name="constant" value="0.35"/></module>
	
</insertBefore>

<insertBefore xpath="/*/terrain_generators/*[@name='vanilla']/*[@name='wtr01_ctrlSide']">
	<module name="wtr01_riverSplitter" type="LargerOutput">
		<property name="sourceModule1" value="wav_z512"/>
		<property name="sourceModule2" value="wav_x512"/>
	</module>
	<module name="wtr01_ctrl" type="Select">
		<property name="sourceModule1" value="wtr01_ctrl_addonX"/>
		<property name="sourceModule2" value="val_p0_35"/>
		<property name="controlModule" value="wtr01_riverSplitter"/>
		<property name="bounds" value="0.8, 100"/>
		<property name="edgeFalloff" value="0.2"/>
	</module>
</insertBefore>

<set xpath="/*/terrain_generators/*[@name='vanilla']/*[@name='wtr01_ctrl'][1]/@name|/*/trash">
wtr01_ctrl_addonX</set>
</configs>
